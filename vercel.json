{
    "version": 2,
    "builds": [
        { "src": "api/*.js", "use": "@vercel/node" },
        { "src": "public/**/*", "use": "@vercel/static" }
      ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/app.js" },
      { "src": "/chiphi.html", "dest": "/public/chiphi.html" },
      { "src": "/", "dest": "/public/hello.html" }
    ]
}


  
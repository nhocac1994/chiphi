
  {
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/public/hello.html"
      },
      {
        "src": "/chiphi.html",
        "dest": "/public/chiphi.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/app.js"
      }
    ]
  }
  
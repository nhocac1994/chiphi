{
    "builds": [
      { "src": "app.js", "use": "@vercel/node" },
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/app.js" },
      { "src": "/chiphi.html", "dest": "/public/chiphi.html" },
      { "src": "/(.*)", "dest": "/public/$1" },
      { "src": "/", "dest": "/public/hello.html" }
    ]
  }
  


  